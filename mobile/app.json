{
  "expo": {
    "name": "Breakpoint Billiards",
    "slug": "breakpoint-billiards",
    "scheme": "breakpoint-billiards",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-black.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.breakpoint.billiards",
      "buildNumber": "120",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Breakpoint Billiards uses your camera to let you take a new profile photo.",
        "NSPhotoLibraryUsageDescription": "Breakpoint Billiards uses your photo library to let you select an existing photo for your profile."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "package": "com.breakpoint.billiards",
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/icon.png",
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "androidNavigationBar": {
        "backgroundColor": "#121212",
        "barStyle": "light-content",
        "softwareKeyboardLayoutMode": "adjustResize"
      },
      "versionCode": 111
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/splash-black.png",
          "imageWidth": 200
        }
      ],
      "expo-router",
      "expo-font",
      "expo-apple-authentication",
      "expo-navigation-bar",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3966e8df-bb98-4bf1-8f2e-ea5310944899"
      }
    }
  }
}